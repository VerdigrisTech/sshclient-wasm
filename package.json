{
  "name": "sshclient-wasm",
  "version": "0.1.4",
  "description": "WebAssembly-based SSH client for the browser with packet interception hooks",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.esm.js",
      "require": "./dist/next.cjs.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.esm.js",
      "require": "./dist/vite.cjs.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.esm.js",
      "require": "./dist/react.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:wasm": "goreleaser build",
    "build:wasm:dev": "goreleaser build --snapshot --clean",
    "build:wasm:release": "goreleaser build --clean --skip=validate",
    "build:ts": "vite build",
    "build": "pnpm run build:wasm",
    "build:dev": "pnpm run build:wasm:dev",
    "build:release": "pnpm run build:wasm:release",
    "clean": "rm -rf dist/",
    "prepublishOnly": "pnpm run build:release",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "pnpm build:dev && vite",
    "dev:vite": "vite"
  },
  "keywords": [
    "ssh",
    "wasm",
    "webassembly",
    "browser",
    "client",
    "websocket",
    "packet"
  ],
  "author": "Verdigris Technologies, Inc. <engineering@verdigris.co>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/VerdigrisTech/sshclient-wasm.git"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@changesets/types": "^6.1.0",
    "@goreleaser/goreleaser": "^2.14.1",
    "@types/node": "^20.11.24",
    "protobufjs-cli": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "dependencies": {
    "protobufjs": "^8.0.0"
  },
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "validator@<13.15.20": ">=13.15.20",
      "axios@>=1.0.0 <1.12.0": ">=1.12.0",
      "tar@<=7.5.3": ">=7.5.4",
      "lodash@>=4.0.0 <=4.17.22": ">=4.17.23",
      "tar@<7.5.7": ">=7.5.7",
      "validator@<13.15.22": ">=13.15.22",
      "js-yaml@<3.14.2": ">=3.14.2",
      "markdown-it@>=13.0.0 <14.1.1": ">=14.1.1",
      "axios@>=1.0.0 <=1.13.4": ">=1.13.5",
      "tar@<=7.5.2": ">=7.5.3",
      "tar@<7.5.8": ">=7.5.8",
      "ajv@<6.14.0": ">=6.14.0",
      "minimatch@<3.1.3": ">=3.1.3",
      "minimatch@>=5.0.0 <5.1.7": ">=5.1.7",
      "minimatch@>=9.0.0 <9.0.6": ">=9.0.6"
    }
  }
}
