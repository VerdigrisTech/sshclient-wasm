{
  "name": "sshclient-wasm",
  "version": "1.0.0",
  "description": "WebAssembly-based SSH client for the browser with packet interception hooks",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "wasm_exec.js"
  ],
  "scripts": {
    "build:wasm": "make build",
    "build:ts": "vite build",
    "build": "npm run build:wasm && npm run build:ts",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "npm run build:wasm && vite",
    "dev:vite": "vite"
  },
  "keywords": [
    "ssh",
    "wasm",
    "webassembly",
    "browser",
    "client",
    "websocket",
    "packet"
  ],
  "author": "Andrew",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrew/sshclient-wasm.git"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/protobufjs": "^6.0.0",
    "protobufjs-cli": "^1.1.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "dependencies": {
    "protobufjs": "^7.5.4"
  }
}