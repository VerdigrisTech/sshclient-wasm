{
  "name": "sshclient-wasm",
  "version": "0.1.4",
  "description": "WebAssembly-based SSH client for the browser with packet interception hooks",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./next": {
      "import": "./dist/next.esm.js",
      "require": "./dist/next.cjs.js",
      "types": "./dist/next.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.esm.js",
      "require": "./dist/vite.cjs.js",
      "types": "./dist/vite.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.cjs.js",
      "types": "./dist/react.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:wasm": "goreleaser build",
    "build:wasm:dev": "goreleaser build --snapshot --clean",
    "build:wasm:release": "goreleaser build --clean --skip=validate",
    "build:ts": "vite build",
    "build": "pnpm run build:wasm",
    "build:dev": "pnpm run build:wasm:dev",
    "build:release": "pnpm run build:wasm:release",
    "clean": "rm -rf dist/",
    "prepublishOnly": "pnpm run build:release",
    "test": "vitest",
    "test:run": "vitest run",
    "dev": "pnpm build:dev && vite",
    "dev:vite": "vite"
  },
  "keywords": [
    "ssh",
    "wasm",
    "webassembly",
    "browser",
    "client",
    "websocket",
    "packet"
  ],
  "author": "Verdigris Technologies, Inc. <engineering@verdigris.co>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/VerdigrisTech/sshclient-wasm.git"
  },
  "devDependencies": {
    "@goreleaser/goreleaser": "^2.9.0",
    "@types/node": "^20.11.24",
    "@types/protobufjs": "^6.0.0",
    "protobufjs-cli": "^1.1.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "dependencies": {
    "@changesets/cli": "^2.29.6",
    "@changesets/types": "^6.1.0",
    "protobufjs": "^7.5.4"
  }
}